<div class="form-inputs">
  <h1>Create Gallery</h1>

  <%= simple_form_for @gallery do |f| %>
    <%= f.input :name %>
    <%= f.input :photos, as: :file, input_html: { multiple: true }%>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success cgb  mt-4" %>
  </div>
  <% end %>
</div>
