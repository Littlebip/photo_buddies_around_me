<div class="form-inputs">
  <h1>Create Gallery</h1>

  <%= simple_form_for @gallery, html: {multipart: true} do |f| %>
    <%= f.input :name %>
    <%= f.file_field :photos, multiple: true %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success cgb  mt-4" %>
  </div>
  <% end %>
</div>
